@model Parcial1_PWA.Models.Subjects

@{
    ViewData["Title"] = "Delete Teacher";
}

<h1>Delete Teacher</h1>

<h3>Are you sure you want to delete this teacher?</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">SubjectName</dt>
        <dd class="col-sm-10">@Model.SubjectName</dd>
        <dt class="col-sm-2">VU</dt>
        <dd class="col-sm-10">@Model.VU</dd>
        <dt class="col-sm-2">Active</dt>
        <dd class="col-sm-10">@(Model.IsActive ? "Yes" : "No")</dd>
    </dl>
</div>
<form asp-action="Delete">
    <input type="hidden" asp-for="SubjectId" />
    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>